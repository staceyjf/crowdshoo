<%- include('../partials/header') %>

<div class="container-fluid p-5 my-0 bg-dark text-white">
    <h1><%= title %></h1>
    <ul class="list-group">
      <% if (venues.length > 0) { %>
        <% venues.forEach((v) => { %>
          <li class="list-group-item">
            <%= v.venueName %><br>
            <%= v.myFavCategory %><br>
            <%= v.averageStarRanking %><br>
            <a href="/venues/<%= v._id %>" class="btn btn-primary">Details</a>
          </li>
        <% }); %>
      <% }; %>
    </ul>
    <br>
</div>


<%- include('../partials/footer') %>