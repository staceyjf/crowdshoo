<%- include('../partials/header') %>

<header>
  <h1 class="site-heading text-center text-faded d-none d-lg-block">
      <span class="site-heading-upper text-primary mb-3">Explore</span>
      <span class="site-heading-lower"><%= title %></span>
  </h1>
</header>

  <!---ADD MAP HERE-->
  <div class="container-fluid p-5 my-0 bg-dark text-white">

<section class="page-section about-heading">
  <div class="container">
      <img class="img-fluid rounded about-heading-img mb-3 mb-lg-0" src="/public/images/intro.jpg" alt="lady at coffee shop" />
      <div class="about-heading-content">
          <div class="row">
              <div class="col-xl-9 col-lg-10 mx-auto">
                  <div class="bg-faded rounded p-5">
                      <ul class="list-group">
                        <% if (avgStars_CatByVenueName.length > 0) { %>
                          <% avgStars_CatByVenueName.forEach((v) => { %>
                            <li class="list-group-item">
                              <%= v.venueName %><br>
                              <%= v.myFavCat %><br>
                              <%= v.stars %><br>
                            </li>
                          <% }); %>
                        <% }; %>
                      </ul>
                  </div>
              </div>
          </div>
      </div>
  </div>
</section>
</div>

<%- include('../partials/footer') %>